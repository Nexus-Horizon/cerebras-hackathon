# Qwen API Configuration
# Copy this file to .env and fill in your actual values

# API Endpoint (optional - defaults to local endpoint)
QWEN_API_URL=http://localhost:8000/qwen/predict

# API Key for external Qwen services (required for external APIs)
QWEN_API_KEY=your_qwen_api_key_here

# Whether API key is required for local endpoint (default: false)
QWEN_API_KEY_REQUIRED=false

# Debug mode for Qwen API calls (default: false)
DEBUG_QWEN=false

# Timeout for API calls in seconds (default: 30)
QWEN_API_TIMEOUT=30

# Model parameters
QWEN_MAX_TOKENS=100
QWEN_TEMPERATURE=0.7

# Cerebras Qwen Configuration
# Set to true to use Cerebras Qwen instead of other Qwen implementations
USE_CEREBRAS_QWEN=false

# Cerebras API Configuration
CEREBRAS_API_URL=https://api.cerebras.com/v1/chat/completions
CEREBRAS_API_KEY=your_cerebras_api_key_here
CEREBRAS_MODEL_NAME=qwen-7b
CEREBRAS_MAX_TOKENS=100
CEREBRAS_TEMPERATURE=0.7
CEREBRAS_TIMEOUT=30

# Debug mode for Cerebras API calls (default: false)
DEBUG_CEREBRAS=false 